+			"id"	=> "fb",
+			"name"	=> "Facebook",
+			"title"	=> t("Facebook Account"),
+			"image"	=> "facebook.png",
+			"remove"=> array(
+				"class"	=> "fb_remove_account",
+			),
+			"add"	=> array(
+				"multiple"	=> false,
+				"class"		=> "fb_link_account",
+			),
+		),
+	);
+}
