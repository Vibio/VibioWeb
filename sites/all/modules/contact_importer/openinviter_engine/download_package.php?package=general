<script>
function go(url)
	{
	window.location=url;
	}
function hovering(element)
	{
	element.style.backgroundImage="url('/new_tb/images/btn_active.jpg')";
	}
function leaving(element,type)
	{
	if (type==1)
		element.style.backgroundImage="url('/new_tb/images/btn_special.jpg')";
	else
		if (type==2)
			element.style.backgroundImage="url('/new_tb/images/btn_active.jpg')";
		else
			element.style.backgroundImage="url('')";
	}
</script>


