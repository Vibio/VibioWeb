+function offer2buy_menu()
+{
+	return array(
+		"offer2buy"					=> array(
+			"title"				=> "Offer2Buy Dashboard",
+			"page callback"		=> "offer2buy_dashboard",
+			"access arguments"	=> array("offer2buy offer"),
+			"type"				=> MENU_NORMAL_ITEM,
+		),
+		"offer2buy/%"				=> array(
