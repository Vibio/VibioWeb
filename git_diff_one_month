+          'action' => 'imagecache_scale_and_crop',
+          'data' => array(
+            'width' => '40',
+            'height' => '40',
+          ),
+        ),
+      ),
+    ),
+    'product_fixed_width' => array(
+      'presetname' => 'product_fixed_width',
+      'actions' => array(
+        '0' => array(
+          'weight' => '0',
+          'module' => 'imagecache',
+          'action' => 'imagecache_scale',
+          'data' => array(
+            'width' => '160px',
+            'height' => '',
+            'upscale' => 0,
+          ),
+        ),
+      ),
+    ),
+    'product_fixed_width_teaser' => array(
+      'presetname' => 'product_fixed_width_teaser',
+      'actions' => array(
+        '0' => array(
+          'weight' => '0',
+          'module' => 'imagecache',
+          'action' => 'imagecache_scale',
+          'data' => array(
+            'width' => '80px',
+            'height' => '',
+            'upscale' => 1,
+          ),
+        ),
+      ),
+    ),
+  );
+  return $items;
+}
