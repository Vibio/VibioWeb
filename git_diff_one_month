+				}
+				
+				span.firefile-install {
+					border: 1px solid #CCCCCC;
+					padding: 2px 4px 0px 4px;
+					text-transform: uppercase;
+					background: #EEEEEE;
+				}
+				
+				span.firefile-install a {
+					color: #444444;
+					font-weight: bold;
+					text-decoration: none;
+				}
