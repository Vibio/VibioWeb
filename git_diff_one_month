-			LIMIT 1";
-	if (!$nid = db_result(db_query($sql, $cid, $access)))
-	{
-		return collection_default_image();
-	}
-	
-	$image = _vibio_item_get_image($nid);
-	return $image == _vibio_item_default_image() ? collection_default_image() : $image;
-}
-
