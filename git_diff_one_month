+; $Id: $
+name = "Vibio Form Beautifier"
+description = "A Collection of jQuery Scripts that can make your forms look good."
+core = 6.x
+package = Other
+
+version = "6.x-1.0-alpha1"
+core = "6.x"
+project = "vibio_form_beautifier"
+datestamp = "1258514431"
