+<?php
+
+/**
+ * Implementation of hook_content_default_fields().
+ */
+function cck_and_menus_from_v1_1_content_default_fields() {
+  $fields = array();
+
+  // Exported field: field_buy_actions
+  $fields['offer-field_buy_actions'] = array(
+    'field_name' => 'field_buy_actions',
+    'type_name' => 'offer',
+    'display_settings' => array(
+      'weight' => '8',
+      'parent' => '',
+      'label' => array(
+        'format' => 'inline',
+      ),
+      'teaser' => array(
+        'format' => 'default',
+        'exclude' => 0,
+      ),
+      'full' => array(
+        'format' => 'default',
+        'exclude' => 0,
+      ),
+      '4' => array(
+        'format' => 'default',
+        'exclude' => 0,
+      ),
+      '2' => array(
+        'format' => 'default',
+        'exclude' => 0,
+      ),
+      '3' => array(
+        'format' => 'default',
+        'exclude' => 0,
+      ),
+      'token' => array(
+        'format' => 'default',
+        'exclude' => 0,
