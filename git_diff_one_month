+		"#type"			=> "fieldset",
+		"#collapsible"	=> true,
+		"#collapsed"	=> false,
+		"#tree"			=> true,
+		"cid"			=> array(
+			"#type"			=> "select",
+			"#required"		=> true,
+			"#title"		=> t("Collection"),
+			"#options"		=> collection_options(),
+			"#default_value"=> $default,
+			"#multiple"		=> true,
+		),
+		/*"new_collection"=> array(
+			"#type"			=> "textfield",
+			"#title"		=> t("New Collection"),
+			"#description"	=> t("Enter a name for this collection"),
+		),*/
+	);
